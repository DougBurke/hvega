{
    "mark": {
        "interpolate": "monotone",
        "type": "line"
    },
    "data": {
        "url": "https://vega.github.io/vega-lite/data/seattle-temps.csv"
    },
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "encoding": {
        "x": {
            "field": "date",
            "timeUnit": "month",
            "type": "temporal"
        },
        "y": {
            "field": "temp",
            "aggregate": "mean",
            "type": "quantitative"
        }
    }
}