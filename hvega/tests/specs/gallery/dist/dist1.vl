{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "data": {
        "url": "https://vega.github.io/vega-lite/data/movies.json"
    },
    "description": "Simple histogram of IMDB ratings.",
    "encoding": {
        "x": {
            "bin": true,
            "field": "IMDB_Rating",
            "type": "quantitative"
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative"
        }
    },
    "mark": {
        "binSpacing": 0,
        "type": "bar"
    }
}