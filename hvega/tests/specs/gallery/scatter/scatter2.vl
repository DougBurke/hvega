{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "data": {
        "url": "https://vega.github.io/vega-lite/data/seattle-weather.csv"
    },
    "description": "Shows the distribution of a single variable (precipitation) using tick marks.",
    "encoding": {
        "x": {
            "field": "precipitation",
            "type": "quantitative"
        }
    },
    "mark": "tick"
}