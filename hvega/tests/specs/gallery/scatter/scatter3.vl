{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "data": {
        "url": "https://vega.github.io/vega-lite/data/cars.json"
    },
    "description": "Shows the relationship between horsepower and the number of cylinders using tick marks.",
    "encoding": {
        "x": {
            "field": "Horsepower",
            "type": "quantitative"
        },
        "y": {
            "field": "Cylinders",
            "type": "ordinal"
        }
    },
    "mark": "tick"
}