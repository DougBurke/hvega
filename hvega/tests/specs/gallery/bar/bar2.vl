{
    "mark": {
        "binSpacing": 0,
        "type": "bar"
    },
    "data": {
        "url": "https://vega.github.io/vega-lite/data/movies.json"
    },
    "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
    "encoding": {
        "x": {
            "field": "IMDB_Rating",
            "bin": true,
            "type": "quantitative"
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative"
        }
    },
    "description": "Simple histogram of IMDB ratings."
}