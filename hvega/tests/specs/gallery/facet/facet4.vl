{
    "mark": "bar",
    "data": {
        "url": "https://vega.github.io/vega-lite/data/cars.json"
    },
    "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
    "encoding": {
        "row": {
            "field": "Origin",
            "type": "ordinal"
        },
        "x": {
            "field": "Horsepower",
            "bin": {
                "maxbins": 15
            },
            "type": "quantitative"
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative"
        }
    },
    "description": "Disitributions of car engine power for different countries of origin"
}