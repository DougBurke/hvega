{
    "mark": "bar",
    "data": {
        "url": "https://vega.github.io/vega-lite/data/cars.json"
    },
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "encoding": {
        "row": {
            "field": "Origin",
            "title": "Origin facet",
            "type": "ordinal"
        },
        "x": {
            "field": "Horsepower",
            "bin": {
                "maxbins": 15
            },
            "type": "quantitative"
        },
        "y": {
            "aggregate": "count",
            "type": "quantitative"
        }
    },
    "description": "Distributions of car engine power for different countries of origin"
}