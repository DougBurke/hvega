{
    "transform": [
        {
            "filter": "datum.symbol === 'GOOG'"
        }
    ],
    "mark": {
        "line": true,
        "point": true,
        "type": "area"
    },
    "data": {
        "url": "https://vega.github.io/vega-lite/data/stocks.csv"
    },
    "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
    "encoding": {
        "x": {
            "field": "date",
            "type": "temporal",
            "axis": {
                "format": "%Y"
            }
        },
        "y": {
            "field": "price",
            "type": "quantitative"
        }
    },
    "description": "Area chart with overlaid lines and point markers"
}