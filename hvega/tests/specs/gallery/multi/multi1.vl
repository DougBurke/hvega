    },
    "vconcat": [
        {
            "mark": "area",
            "width": 500,
            "encoding": {
                "x": {
                    "field": "date",
                    "scale": {
                        "domain": {
                            "selection": "myBrush"
                        }
                    },
                    "type": "temporal",
                    "axis": {
                        "title": null
                    }
                },
                "y": {
                    "field": "price",
                    "type": "quantitative"
                }
            }
        },
        {
            "height": 60,
            "mark": "area",
            "width": 480,
            "selection": {
                "myBrush": {
                    "encodings": [
                        "x"
                    ],
                    "type": "interval"
                }
            },
            "encoding": {
                "x": {
                    "field": "date",
                    "type": "temporal",
                    "axis": {
                        "format": "%Y"
                    }
                },
                "y": {
                    "field": "price",
                    "type": "quantitative",
                    "axis": {
                        "tickCount": 3,
                        "grid": false
                    }
                }
            }
        }
