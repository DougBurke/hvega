{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "data": {
        "url": "https://vega.github.io/vega-lite/data/cars.json"
    },
    "encoding": {
        "x": {
            "field": "Horsepower",
            "type": "quantitative"
        },
        "y": {
            "field": "Miles_per_Gallon",
            "type": "quantitative"
        }
    },
    "height": 300,
    "mark": "circle",
    "selection": {
        "myZoomPan": {
            "bind": "scales",
            "clear": "click[event.shiftKey]",
            "type": "interval"
        }
    },
    "width": 300
}