{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "config": {
        "legend": {
            "disable": true
        }
    },
    "data": {
        "url": "https://vega.github.io/vega-lite/data/cars.json"
    },
    "encoding": {
        "color": {
            "field": "Origin",
            "type": "nominal"
        },
        "opacity": {
            "field": "Weight_in_lbs",
            "type": "quantitative"
        },
        "size": {
            "field": "Horsepower",
            "type": "quantitative"
        },
        "x": {
            "field": "Horsepower",
            "type": "quantitative"
        },
        "y": {
            "field": "Miles_per_Gallon",
            "type": "quantitative"
        }
    },
    "height": 300,
    "mark": "circle",
    "width": 300
}