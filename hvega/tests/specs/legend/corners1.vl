{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "config": {
        "legend": {
            "cornerRadius": 5,
            "fillColor": "pink",
            "labelAlign": "right",
            "labelBaseline": "top",
            "labelColor": "fireBrick",
            "labelFontSize": 20,
            "labelFontWeight": 600,
            "labelOpacity": 0.6,
            "labelOverlap": "greedy",
            "padding": 30,
            "rowPadding": 15,
            "strokeColor": "orange",
            "strokeDash": [
                4,
                2,
                6,
                1
            ],
            "strokeWidth": 2,
            "symbolStrokeColor": null,
            "symbolStrokeWidth": 1,
            "titlePadding": 20
        }
    },
    "data": {
        "url": "https://vega.github.io/vega-lite/data/cars.json"
    },
    "encoding": {
        "color": {
            "field": "Origin",
            "type": "nominal"
        },
        "opacity": {
            "field": "Weight_in_lbs",
            "type": "quantitative"
        },
        "size": {
            "field": "Horsepower",
            "type": "quantitative"
        },
        "x": {
            "field": "Horsepower",
            "type": "quantitative"
        },
        "y": {
            "field": "Miles_per_Gallon",
            "type": "quantitative"
        }
    },
    "height": 300,
    "mark": "circle",
    "width": 300
}