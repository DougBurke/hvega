{
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "config": {
        "range": {
            "ramp": {
                "scheme": "reds"
            }
        }
    },
    "data": {
        "url": "https://vega.github.io/vega-lite/data/cars.json"
    },
    "encoding": {
        "color": {
            "bin": true,
            "field": "Acceleration",
            "type": "quantitative"
        },
        "x": {
            "field": "Horsepower",
            "type": "quantitative"
        },
        "y": {
            "field": "Miles_per_Gallon",
            "type": "quantitative"
        }
    },
    "mark": "point"
}