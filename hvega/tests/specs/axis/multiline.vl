{
    "config": {
        "axis": {
            "labelLineHeight": 20
        },
        "lineBreak": " "
    },
    "mark": "point",
    "data": {
        "url": "https://vega.github.io/vega-lite/data/cars.json"
    },
    "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
    "encoding": {
        "color": {
            "field": "Origin",
            "type": "nominal",
            "legend": null
        },
        "x": {
            "field": "Horsepower",
            "type": "quantitative",
            "axis": {
                "labelExpr": "datum.label + ' horses'",
                "labelLineHeight": 22,
                "labelFontSize": 11
            }
        },
        "y": {
            "field": "Miles_per_Gallon",
            "type": "quantitative",
            "axis": {
                "labelExpr": "datum.label+' mpg'",
                "labelFontSize": 22
            }
        }
    }
}